<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Blazored.SessionStorage" Version="2.4.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authorization" Version="6.0.24" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="6.0.24" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.14" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.14" PrivateAssets="all" />
    <PackageReference Include="Radzen.Blazor" Version="4.16.4" />
    <PackageReference Include="supabase-csharp" Version="0.13.7" />
    
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CommonBase\CommonBase.csproj" />
    <ProjectReference Include="..\CommonHomeComponents\CommonHomeComponents.csproj" />
    <ProjectReference Include="..\CommonLoginComponets\CommonLoginComponets.csproj" />
  </ItemGroup>

  <ItemGroup>
  
    <BlazorWebAssemblyLazyLoad Include="CommonLoginComponets.dll" />
    <BlazorWebAssemblyLazyLoad Include="CommonHomeComponents.dll" />
  </ItemGroup>

</Project>
